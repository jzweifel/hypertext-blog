{
  "name": "hypertext-blog",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun run uno:dev\"",
    "start": "bun run uno && bun run src/main.ts",
    "uno": "bunx unocss",
    "uno:dev": "bunx unocss --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "bun-types": "^1.0.3",
    "concurrently": "^8.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typed-html": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/html": "^0.7.0",
    "@elysiajs/static": "^0.7.1",
    "@unocss/transformer-variant-group": "0.55.7",
    "elysia": "^0.7.15",
    "unocss": "0.55.7"
  }
}
